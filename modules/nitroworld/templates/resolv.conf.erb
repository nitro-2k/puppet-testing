<% if @search[0] != 'UNSET' -%>
<% if @search.class == String -%>
search <%= @search %>
<% else -%>
search <%= @search.join(' ') %>
<% end -%>
<% end -%>
<% if @domain != 'UNSET' and @search[0] == 'UNSET' -%>
domain <%= @domain %>
<% end -%>
<% if @nameservers.class == String -%>
nameserver <%= @nameservers %>
<% else -%>
<% @nameservers.each do |nameserver| -%>
nameserver <%= nameserver %>
<% end -%>
<% end -%>
